# Backend Environment Variables Example
# Copy this file to .env and fill in your actual values

# AI API Keys (Choose one or both - Gemini is preferred if both are set)
OPENAI_API_KEY=your-openai-api-key-here
GEMINI_API_KEY=your-gemini-api-key-here

# TMDB API Key (for movie data)
TMDB_API_KEY=your-tmdb-api-key-here

# AI Provider Selection (gemini or openai)
AI_PROVIDER=gemini

# Database Configuration (Optional - AI features work without database)
SPRING_DATASOURCE_URL=jdbc:postgresql://localhost:5433/movies_tracker
SPRING_DATASOURCE_USERNAME=postgres
SPRING_DATASOURCE_PASSWORD=your-database-password

# Server Configuration
SERVER_PORT=8080
